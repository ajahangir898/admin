# Production Environment Variables
# Replace with your actual production domain
VITE_API_BASE_URL=https://systemnextit.com
VITE_PRIMARY_DOMAIN=systemnextit.com
VITE_DEFAULT_TENANT_SLUG=default
VITE_REMOTE_SAVE_DEBOUNCE_MS=1200
VITE_DISABLE_REMOTE_SAVE=false

# ====================================================
# CDN Configuration (optional - enables CDN for static assets and images)
# ====================================================
# VITE_CDN_ENABLED=true
# VITE_CDN_PROVIDER=cloudflare  # Options: cloudflare, cloudfront, bunnycdn, custom
# VITE_CDN_BASE_URL=https://cdn.systemnextit.com
# VITE_CDN_IMAGE_URL=https://cdn.systemnextit.com  # Optional, defaults to CDN_BASE_URL
# VITE_CDN_STATIC_URL=https://cdn.systemnextit.com  # Optional, defaults to CDN_BASE_URL
# VITE_CDN_IMAGE_TRANSFORM=true  # Enable CDN image transformations
# VITE_CDN_IMAGE_QUALITY=80
# VITE_CDN_IMAGE_FORMAT=auto  # Options: auto, webp, avif, original
