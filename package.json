{
  "$schema": "https://json.schemastore.org/package",
  "name": "e-commerce-solutions",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server && npm run copy:html",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr entry-server.tsx --outDir dist/server",
    "copy:html": "cp dist/client/index.html dist/client/index.html.bak 2>/dev/null || true",
    "start": "NODE_ENV=production node server.js",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@google/genai": "^0.7.0",
    "@google/generative-ai": "^0.24.1",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "compression": "^1.8.1",
    "express": "^4.22.1",
    "lucide-react": "0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.1",
    "react-loading-skeleton": "^3.5.0",
    "recharts": "2.12.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.14.0",
    "@types/socket.io": "^3.0.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.1.4"
  }
}
